<md-sidenav md-component-id="left" class="md-sidenav-left" hide show-xs>
    <md-toolbar>
        <span class="md-headline md-toolbar-tools">รายการ</span>
    </md-toolbar>
    <md-content layout-padding>
        <md-list class="md-list custom">
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isHome}"  ng-click="layout.navigate('/')" aria-label="หน้าหลัก   ">
                    หน้าหลัก </md-button>
            </md-list-item>
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isAboutUs}"  ng-click="layout.navigate('/aboutus')" aria-label="เกี่ยวกับโครงการ">
                    เกี่ยวกับโครงการ</md-button>
            </md-list-item>
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isKnowledgePeople}"  ng-click="layout.navigate('/knowledge/people') " aria-label="ความรู้สำหรับ ประชาชน ">
                    ความรู้สำหรับ ประชาชน
                </md-button>
            </md-list-item>
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isKnowledgeHealthCare}"  ng-click="layout.navigate('/knowledge/healthcare')" aria-label="ความรู้สำหรับ บุคลากรทางการแพทย์">
                    ความรู้สำหรับ บุคลากรทางการแพทย์
                </md-button>
            </md-list-item>
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isMember}" ng-click="layout.navigate('/member') " aria-label="สำหรับคณะทำงาน UHOSNET">
                    สำหรับคณะทำงาน UHOSNET
                </md-button>
            </md-list-item>
            <md-divider></md-divider>
            <md-list-item>
                <md-button ng-class="{'md-primary': current.page.isContactUs}" ng-click="layout.navigate('/contactus')" aria-label="ติดต่อเรา">
                    ติดต่อเรา</md-button>
            </md-list-item>
        </md-list>
    </md-content>
</md-sidenav>